{
  "manifest_version": 3,
  "name": "block-yt-content",
  "version": "1.0.0",
  "description": "Block YouTube content",
  "permissions": ["activeTab"],
  "host_permissions": ["https://www.youtube.com/*"],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "css": ["index.css"]
    }
  ]
}
